<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.kgc.entity.Allqu" table="allqu" schema="5173game">
        <id name="qid">
            <column name="qid" sql-type="int(10)"/>
        </id>
        <property name="name">
            <column name="name" sql-type="varchar(20)" length="20" not-null="true"/>
        </property>
        <property name="gid">
            <column name="gid" sql-type="int(10)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>